# Define required providers
terraform {
  required_providers {
    openstack = {
      source  = "terraform-provider-openstack/openstack"
      version = "~> 3.0.0"
    }
    kubernetes = {
      source = "hashicorp/kubernetes"
      version = "2.34.0"
    }
  }
}



provider "kubernetes" {
  config_path = "../config/${var.k8s_cluster}/config"
  config_context = "${var.k8s_cluster}"
}